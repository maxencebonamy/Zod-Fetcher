{
	"prettier.enable": false,
	"editor.formatOnSave": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/node_modules/**": true
	},
	"json.schemas": [
		{
			"url": "https://cdn.jsdelivr.net/npm/tsup/schema.json",
			"fileMatch": [
				"tsup.config.json"
			]
		}
	]
}