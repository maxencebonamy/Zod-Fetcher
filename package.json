{
	"name": "zod-rest-fetch",
	"version": "1.0.0-beta.7",
	"description": "Fetch utilities for REST APIs using Zod",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsup",
		"build:prod": "pnpm run lint && pnpm run test && pnpm run build",
		"start": "node dist/index.js",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [
		"zod",
		"rest",
		"fetch",
		"api",
		"utilities",
		"typescript"
	],
	"author": "Maxence Bonamy",
	"license": "ISC",
	"dependencies": {
		"zod": "^3.23.8"
	},
	"peerDependencies": {
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@maxencebonamy/eslint-plugin": "1.0.1",
		"@types/node": "^20.13.0",
		"eslint": "8.57.0",
		"tsup": "^8.1.0",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	},
	"engines": {
		"node": ">=14.0.0",
		"pnpm": ">=6.0.0"
	}
}